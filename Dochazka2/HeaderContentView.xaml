<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Dochazka2.HeaderContentView"
    xmlns:pages="clr-namespace:Dochazka2"
    HeightRequest="100">

    <StackLayout BackgroundColor="{AppThemeBinding Light={StaticResource pagecolorlight}, Dark={StaticResource pagecolordark}}">
        <Image Source="aplex"
               WidthRequest="100"
               HorizontalOptions="Start"/>
    </StackLayout>
</ContentView>
